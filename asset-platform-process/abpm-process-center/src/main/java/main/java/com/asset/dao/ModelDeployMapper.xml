<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.asset.dao.ModelDeployMapper">

    <select id="getDeployID" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT
          ID_
        FROM
          act_re_deployment
        WHERE
          KEY_ = #{appDefKey,jdbcType=VARCHAR};
    </select>


</mapper>
